		<annotation>
			<documentation>Root element for data not related to a patient</documentation>
		</annotation>
		<complexType>
			<sequence>
				<element name="Brukere">
					<complexType>
						<sequence>
							<element name="Helseperson" maxOccurs="unbounded">
								<complexType>
									<sequence>
										<element name="Id" type="string"/>
