									<element name="DatoDosekontroll" type="date" />
								</sequence>
							</complexType>
						</element>
						<element name="ForholdsregelVedInntak" type="string" minOccurs="0"/>
						<element name="BegrunnelseReservasjonGeneriskBytte" type="string" minOccurs="0"/>
						<element name="BegrunnelseCaveOverstyring" type="string" minOccurs="0"/>
						<element name="ReasonForChange" type="string" minOccurs="0"/>
						<element name="ConfirmedIsNotFertile" type="boolean" minOccurs="0"/>
            <element name="PreparationId" type="string" minOccurs="0"/>
