        <element name="RefForrigePrescription" type="string" minOccurs="0"/>
        <element name="PrescriptionVersionId" type="string" minOccurs="0"/>
        <element name="RefForrigePrescriptionVersion" type="string" minOccurs="0"/>
        <element name="ForskrivningsInfoId" type="string">
          <annotation>
            <documentation>Legemiddelbehandling/PrescriptionDetails/ForskrivningsInfo[]/Id</documentation>
          </annotation>
        </element>
        <element name="Tidspunkt" type="dateTime"/>
