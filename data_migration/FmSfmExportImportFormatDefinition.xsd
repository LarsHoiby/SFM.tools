                          Registered by
                          Approved by
                          Forskriver
                          Created By
                          LastedEdited By
                          Signed by
                        </documentation>
                      </annotation>
                    </element>
									</sequence>
								</complexType>
							</element>
						</sequence>
					</complexType>
				</element>
        <element ref="export:Seponering" minOccurs="0"/>
      </sequence>
		</complexType>
