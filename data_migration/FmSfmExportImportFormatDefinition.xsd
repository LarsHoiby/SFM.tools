                    <element name="ForskrivningsrettAB" type="string" minOccurs="0">
                      <annotation>
                        <documentation>
                          All
                          NotA
                          NotAB
                        </documentation>
                      </annotation>
                    </element>
									</sequence>
								</complexType>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="Institusjon" maxOccurs="unbounded">
					<complexType>
						<sequence>
							<element name="Id" type="string"/>
							<element name="Navn" type="string"/>
							<element name="Ident" type="export:Ident" maxOccurs="unbounded"/>
							<element name="Address" type="export:Address" minOccurs="0"/>
							<element name="TeleCom" type="export:TeleCom" minOccurs="0" maxOccurs="unbounded"/>
						</sequence>
					</complexType>
				</element>
				<element name="ForskrivningsMaler" minOccurs="0">
					<complexType>
						<sequence>
							<element name="Mal" maxOccurs="unbounded">
								<complexType>
									<sequence>
										<element name="Id" type="string"/>
										<element name="Navn" type="string"/>
