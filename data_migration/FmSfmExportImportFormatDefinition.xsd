										<element name="Name" type="string"/>
										<element name="Description" type="string"/>
										<element name="Created" type="dateTime"/>
										<choice minOccurs="0">
											<element name="BrukerId" type="string"/>
											<element name="InstitusjonsId" type="string"/>
										</choice>
										<element name="Diagnosekode" type="export:CV"/>
										<element name="Behandlingsfase" type="export:CV"/>
										<element name="Oppmerksomhet" type="export:CV"/>
										<element name="DoseresEtter" type="export:CS"/>
										<element name="OvreGrense" type="decimal" minOccurs="0"/>
										<element name="NedreGrense" type="decimal" minOccurs="0"/>
										<element name="Enhet" type="export:CV" minOccurs="0"/>
										<element name="Maler" maxOccurs="unbounded">
											<complexType>
												<sequence>
													<element name="MalId" type="string"/>
												</sequence>
											</complexType>
										</element>
									</sequence>
								</complexType>
							</element>
						</sequence>
					</complexType>
				</element>
				<element name="LokaltLegemiddel" minOccurs="0" maxOccurs="unbounded">
					<complexType>
						<sequence>
							<element name="Id" type="string"/>
							<element ref="export:LokalVare"/>
						</sequence>
					</complexType>
				</element>
				<element name="LokaltMagistreltLegemiddel" minOccurs="0" maxOccurs="unbounded">
