	<element name="LegemiddelForskrivningsInfo">
		<complexType>
			<complexContent>
				<extension base="export:ForskrivningsInfo">
					<sequence>
						<element ref="export:Indikasjon" minOccurs="0"/>
						<element ref="export:ReseptDokLegemiddel"/>
						<element name="AkInformasjon" minOccurs="0">
							<complexType>
								<sequence>
									<element name="Inr" type="decimal" nillable="true" minOccurs="0"/>
									<element name="DatoInrKontroll" type="date" nillable="true" minOccurs="0"/>
									<element name="InrMinimum" type="decimal"/>
									<element name="InrMaximum" type="decimal"/>
									<element name="DatoNesteInrKontroll" type="date" nillable="true" minOccurs="0"/>
