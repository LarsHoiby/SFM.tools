							<element name="Tidspunkt" type="dateTime"/>
						</sequence>
					</complexType>
				</element>
				<element name="Slettet" minOccurs="0">
					<complexType>
						<sequence>
							<element name="Tidspunkt" type="dateTime"/>
							<element name="BrukerId" type="string"/>
