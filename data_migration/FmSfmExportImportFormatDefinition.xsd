			<element name="ForskrivningsType" type="export:CS">
				<annotation>
					<documentation>eRp/uRp (not yet sent or printed)
eRp (e-resept)
uRp (printed)
fRp (fax)
tRp (telephone)
Reg (local registration)
iRp (imported)</documentation>
				</annotation>
			</element>
			<element name="Utloper" type="date" minOccurs="0"/>
